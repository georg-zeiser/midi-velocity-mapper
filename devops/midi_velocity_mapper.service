[Unit]
Description=Midi Velocity Mapper

[Service]
WorkingDirectory=/home/ubuntu/MidiVelocityMapper
ExecStart=/home/ubuntu/dotnet/dotnet /home/ubuntu/MidiVelocityMapper/MidiVelocityMapper.dll --urls "http://*:5000"
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=MidiVelocityMapper
User=ubuntu
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false

[Install]
WantedBy=multi-user.target